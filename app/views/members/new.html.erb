<div class="panel"><div class="panel-body">
<h3>添加用户</h3>

  <%= simple_form_for @user , :url => members_new_path , :method => :get do |f| %>
      <%= f.input :username %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.input :password_confirmation %>
      <%= f.button :submit %>
  <% end %>

</div></div>